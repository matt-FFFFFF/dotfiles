# Get editor completions based on the config schema
"$schema" = 'https://starship.rs/config-schema.json'

format = """\
[ÓÇ∂](fg:#C2B4DF)\
$os\
$sudo\
$shell\
[ÓÇ∞](fg:#C2B4DF bg:#8A5A7F)\
$directory\
[ÓÇ∞](fg:#8A5A7F bg:#8AAE7F)\
$git_branch\
$git_commit\
$git_status\
$git_state\
[ÓÇ∞](fg:#8AAE7F bg:#FBF39F)\
($git_metrics)\
[ÓÇ∞](fg:#FBF39F)\
$fill\
$status\
$all\
$cmd_duration\
$line_break\
$character"""

# disabled due to fill module
# right_format = ''
add_newline = true

[line_break]
disabled = false

[fill]
symbol = ' '

[os]
disabled = false
format = '[$symbol]($style)'
style = "#C2B4DF inverted"

[os.symbols]
Macos = "ÔÖπ "
Windows = "ÔÖ∫ "
Ubuntu = "ÔÖº "

[sudo]
disabled = false
symbol = "üßô"
format = '[$symbol ]($style)'
style = "bg:#C2B4DF"

[shell]
disabled = false
zsh_indicator = 'Óöë'
powershell_indicator = 'ÓöÉ'
style = 'fg:#C2B4DF inverted'
format = '[$indicator ]($style)'

[directory]
format = "[ \uf07c  $path ]($style)"
style = "fg:#E4E4E4 bg:#8A5A7F"

[git_branch]
format = '[ $symbol$branch(:$remote_branch) ]($style)'
symbol = "Óúâ Ôêò "
style = "fg:#1C3A5E bg:#8AAE7F"

[git_status]
format = '([\[$all_status$ahead_behind\]]($style))'
style = "fg:#1C3A5E bg:#8AAE7F"

[git_commit]
disabled = false
format = '[\((#$hash)$tag\)]($style)'
tag_disabled = false
only_detached = true
style = "fg:#1C3A5E bg:#8AAE7F"

[git_metrics]
format = '([ +$added ]($added_style))([-$deleted ]($deleted_style))'
added_style = "fg:#1C3A5E bg:#FBF39F"
deleted_style = "fg:red bg:#FBF39F"
disabled = false

[cmd_duration]
disabled = false
format = "[[ÓÇ∂](fg:#5C7F6A)‚è±Ô∏è $duration[ÓÇ¥](fg:#5C7F6A)]($style)"
style = "#5C7F6A inverted"

[character]
success_symbol = '[‚ùØ](bold #C2B4DF) '
error_symbol = '[‚ùØ](bold purple) '

[time]
disabled = true
time_format = "%R" # Hour:Minute Format
style = "bg:#1d2230"
format = '[[ Û±ëç $time ](bg:#1C3A5E fg:#8DFBD2)]($style)'

[azure]
disabled = false
format = '[ÓÇ∂](fg:#3376CD)[$symbol($subscription)]($style)[ÓÇ¥](fg:#3376CD)'
style = 'fg:#3376CD bold inverted'
symbol = 'Û∞†Ö '

[golang]
disabled = false
format = '[ÓÇ∂](fg:cyan)[$symbol($version )]($style)[ÓÇ¥](fg:cyan)'
style = 'fg:cyan inverted'
symbol = 'Óòß '

[status]
disabled = false
symbol = 'üî¥ '
success_symbol = ''
format = '[$symbol( $common_meaning )$signal_name$maybe_int]($style) '
map_symbol = true

[terraform]
symbol = 'Û±Å¢ '
format = '[ÓÇ∂](fg:105)[$symbol$version( $workspace)]($style)[ÓÇ¥](fg:105)'
style = 'fg:105 inverted'
