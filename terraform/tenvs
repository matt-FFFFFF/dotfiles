# file to be dot sourced to create the required aliases for terraform
export ARM_CLIENT_ID=$(arm-client-id)
export ARM_CLIENT_SECRET=$(arm-client-secret)
export ARM_SUBSCRIPTION_ID=$(arm-subscription-id)
export ARM_TENANT_ID=$(arm-tenant-id)

export AZURERM_CLIENT_ID=$ARM_CLIENT_ID
export AZURERM_CLIENT_SECRET=$ARM_CLIENT_SECRET
export AZURERM_SUBSCRIPTION_ID=$ARM_SUBSCRIPTION_ID
export AZURERM_TENANT_ID=$ARM_TENANT_ID
